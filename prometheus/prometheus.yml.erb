global:
  scrape_interval: 10s
  evaluation_interval: 10s

scrape_configs:
- job_name: 'node'
  honor_labels: true

  static_configs:
  - targets:
    - node-exporter:9100

  relabel_configs:
  - source_labels: [__address__]
    regex: (.*)(?::\d+)
    action: replace
    replacement: $1
    target_label: instance
